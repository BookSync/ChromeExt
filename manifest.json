{
  "manifest_version": 2,

  "name": "Getting started example",
   "content_scripts": [{
     "js": ["jquery.min.js"],
     "matches": ["http://*/*", "https://*/*"]
   }],
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "login.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
